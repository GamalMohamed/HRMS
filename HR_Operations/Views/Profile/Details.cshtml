    <script type="text/javascript">

        document.querySelector('#download-btn').addEventListener('click', function (e) {
            var files = e.target.dataset.files.split(' ');
            multiDownload(files);
        });

        function CheckFileType(fileName) {
            if (fileName === "") {
                alert("Browse to upload a valid File with supported extension");
                return false;
            }
            var ext;
            if (window.navigator.userAgent.indexOf("Edge") > -1) {
                ext = fileName.split(".")[2].toLowerCase();
            } else {
                ext = fileName.split(".")[1].toLowerCase();
            }
            if (ext === "jpg" || ext === "jpeg" || ext === "png" || ext === "pdf")
                return true;
            else {
                alert("File with " + fileName.split(".")[1].toUpperCase() + " format is invalid. Upload a valid file with one of the supported extensions");
                return false;
            }
        }
    </script>

